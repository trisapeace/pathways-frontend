{
  "name": "pathways-frontend",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "@lingui/babel-preset-react": "2.0.1",
    "@lingui/cli": "2.0.1",
    "@types/enzyme": "^3.1.9",
    "@types/enzyme-adapter-react-16": "^1.0.2",
    "@types/expo": "^26.0.0",
    "@types/history": "^4.6.2",
    "@types/jest": "^22.2.2",
    "@types/react": "^16.0.40",
    "@types/react-native": "^0.52.20",
    "@types/react-redux": "^5.0.19",
    "@types/react-test-renderer": "^16.0.1",
    "@types/redux-first-router": "^1.10.2",
    "@types/redux-first-router-link": "^1.4.3",
    "@types/uuid": "^3.4.3",
    "babel-core": "^6.26.2",
    "babel-jest": "^22.4.3",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.3",
    "exp": "^53.1.0",
    "jest": "^22.4.3",
    "jest-expo": "26.0.0",
    "react-devtools": "^3.2.1",
    "react-dom": "^16.3.1",
    "react-native-scripts": "1.13.1",
    "react-test-renderer": "^16.3.1",
    "redux-devtools": "^3.4.1",
    "ts-jest": "^22.4.2",
    "tslint": "^5.9.1",
    "tslint-immutable": "^4.5.3",
    "typescript": "^2.7.2"
  },
  "main": "./node_modules/react-native-scripts/build/bin/crna-entry.js",
  "scripts": {
    "postinstall": "bin/postinstall.sh",
    "lint": "tslint src/**/*.ts src/**/*.tsx",
    "build": "tsc",
    "test": "yarn run build && yarn run jest",
    "clean": "rimraf lib/**/*.js lib/**/*.js.map locale/_build/**/*.json",
    "extract-strings": "yarn run build && lingui extract",
    "build-strings": "lingui compile --strict",
    "clean-strings": "rimraf locale/_build/**/*.json",
    "start": "react-native-scripts start",
    "eject": "react-native-scripts eject",
    "android": "react-native-scripts android",
    "ios": "react-native-scripts ios"
  },
  "jest": {
    "preset": "jest-expo",
    "setupTestFrameworkScriptFile": "<rootDir>/lib/application/__tests__/helpers/setup_tests.js",
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(react-native|expo|@expo/.*|native-base)/)"
    ],
    "testRegex": "/__tests__/[\\w]+\\.(test|spec)\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "dependencies": {
    "@lingui/react": "2.0.1",
    "expo": "^27.0.0",
    "history": "^4.7.2",
    "intl": "^1.2.5",
    "native-base": "^2.4.2",
    "react": "16.3.1",
    "react-native": "^0.55.0",
    "react-redux": "^5.0.7",
    "redux": "^3.7.2",
    "redux-first-router": "^0.0.16-next",
    "redux-first-router-link": "^1.4.2",
    "redux-persist": "^5.9.1",
    "redux-saga": "^0.16.0",
    "uuid": "^3.2.1"
  },
  "lingui": {
    "sourceLocale": "en",
    "format": "lingui",
    "srcPathDirs": [
      "<rootDir>/lib"
    ]
  }
}
